{
  "name": "spaceaware.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "npm run copy:orbpro",
    "deploy": "npm run build && git add -A && git commit -m 'deploy' && git push",
    "dev": "npm run copy:orbpro && vite",
    "start": "npm run copy:orbpro && vite",
    "build": "npm run copy:orbpro && vite build && cp -R ./packages/orbpro/Build/Cesium/* docs/assets",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "copy:orbpro": "rm -R packages/orbpro/Build/* && cp -R ../OrbPro/Build/* packages/orbpro/Build"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tsconfig/svelte": "^5.0.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
