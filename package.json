{
  "name": "spaceaware.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "npm run copy:orbpro",
    "deploy": "npm run build && git add -A && git commit -m 'deploy' && git push",
    "dev": "npm run copy:orbpro && vite",
    "start": "npm run copy:orbpro && vite",
    "build": "rm -Rf docs/assets/* && npm run copy:orbpro && vite build && cp -R ./packages/orbpro/Build/Cesium/* docs/assets && rm ./docs/assets/index.*js",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "copy:orbpro": "rm -R packages/orbpro/Build/* && cp -R ../OrbPro/Build/* packages/orbpro/Build"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tsconfig/svelte": "^5.0.0",
    "@types/d3": "^7.4.2",
    "@types/jquery": "^3.5.25",
    "@types/node": "^20.8.9",
    "autoprefixer": "^10.4.16",
    "devtools-detect": "^4.0.1",
    "postcss": "^8.4.31",
    "rollup-plugin-obfuscator": "^1.0.3",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.5",
    "terser": "^5.23.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "d3": "^7.8.5",
    "datatables.net": "^1.13.6",
    "datatables.net-dt": "^1.13.6",
    "jquery": "^3.7.1",
    "svelte-awesome": "^3.2.1"
  }
}
